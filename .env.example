# Required: Shared secret for dashboard access
DASHBOARD_API_KEY=your-secret-key-here

# Required: GitHub PAT with `repo` + `codespace` scopes
GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Optional: Fly.io token; deployment info skipped if missing
FLY_API_TOKEN=

# Optional: Path to config.yaml (default: config.yaml)
DASHBOARD_CONFIG_PATH=config.yaml
